<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - My Telerik MVC Application</title>
    <!-- Include Kendo UI stylesheets and scripts -->
    <link rel="stylesheet" href="https://cdn.kendostatic.com/2022.3.1207/styles/kendo.common.min.css">
    <link rel="stylesheet" href="https://cdn.kendostatic.com/2022.3.1207/styles/kendo.default.min.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.kendostatic.com/2022.3.1207/js/kendo.all.min.js"></script>
    <!-- Include Kendo UI stylesheets and scripts -->
    <link href="~/Content/css/custom.css" rel="stylesheet" />
    <link href="~/Content/css/Carousel.css" rel="stylesheet" />
    <link href="~/Content/css/Sidebar.css" rel="stylesheet" />
    <link href="https://kendo.cdn.telerik.com/themes/6.4.0/default/default-purple.css" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
</head>
<body class="k-content">
    <main>
        <div class="container-fluid">
            @RenderBody()
        </div>
    </main>
    <script src="https://cdn.kendostatic.com/2023.2.829/js/jquery.min.js"></script>
    <script src="https://cdn.kendostatic.com/2023.2.829/js/jszip.min.js"></script>
    <script src="https://cdn.kendostatic.com/2023.2.829/js/kendo.all.min.js"></script>
    <script src="https://cdn.kendostatic.com/2023.2.829/js/kendo.aspnetmvc.min.js"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="~/Content/js/Carousel.js"></script>
</body>
</html>
