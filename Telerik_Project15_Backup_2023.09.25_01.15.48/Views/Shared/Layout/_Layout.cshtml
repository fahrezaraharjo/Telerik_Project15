<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - My Telerik MVC Application</title>
    <!-- Include jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <!-- Include Kendo UI scripts and styles -->
    <link href="https://kendo.cdn.telerik.com/2021.3.914/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="https://kendo.cdn.telerik.com/2021.3.914/styles/kendo.default.min.css" rel="stylesheet" />
    <script src="https://kendo.cdn.telerik.com/2021.3.914/js/kendo.all.min.js"></script>

    <link href="~/Content/css/custom.css" rel="stylesheet" />
    <link href="~/Content/css/Carousel.css" rel="stylesheet" />
    <link href="~/Content/css/Sidebar.css" rel="stylesheet" />
    <link href="https://kendo.cdn.telerik.com/themes/6.4.0/default/default-purple.css" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
</head>
<body class="k-content">
    <main>
        <div class="container-fluid">
            @RenderBody()
        </div>
    </main>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <script src="~/Content/js/home.js"></script>
</body>
</html>
